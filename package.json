{
  "name": "js-world-better-snippets",
  "displayName": "JS World Better Snippets",
  "description": "This extension provides a collection of JavaScript, TypeScript, React and React Native code snippets to help you write code faster and more efficiently.",
  "version": "0.0.1",
  "author": {
    "name": "victordev",
    "url": "https://github.com/victordev13"
  },
  "publisher": "victordev",
  "icon": "./images/icon.png",
  "keywords": [
    "react",
    "react native",
    "snippets",
    "reactjs",
    "react snippets",
    "react native snippets",
    "javascript",
    "javascript snippets",
    "typescript",
    "typescript snippets"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "path": "./snippets/typescript.code-snippets"
      },
      {
        "path": "./snippets/react.code-snippets"
      },
      {
        "path": "./snippets/react-native.code-snippets"
      }
    ]
  },
  "scripts": {
    "pack": "vsce package",
    "publish": "vsce package && vsce publish",
    "publish:pre-release": "vsce package --pre-release && vsce publish --pre-release",
    "generate-doc": "./scripts/generate-doc.sh"
  },
  "repository": {
    "url": "https://github.com/victordev13/react-better-snippets"
  }
}
